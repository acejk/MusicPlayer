<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ace.musicplayer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".service.MediaPlayerService" />

        <activity android:name=".list.MainView" />
        <activity android:name=".list.ArtistList" />
        <activity android:name=".list.AlbumList" />
        <activity android:name=".list.AllMusicList" />
        <activity android:name=".list.DetailArtistList" />
        <activity android:name=".list.DetailAlbumList" />
        <activity android:name=".play.PlayMusic" />
    </application>

</manifest>
